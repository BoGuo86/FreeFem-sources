include "test.idp"

cout << "det" << endl;
real t;

cout << "matrix of dimension 2" << endl;

t = clock();
real res = det([[-2, 2, -3],
            [-1, 1, 3],
            [2, 0, -1]]);
t = clock() - t;
cout << IND << t << endl;
test(res == 18);

cout << "matrix of dimension 3" << endl;

t = clock();
res = det([[1, 2],
            [3, 4]]);
t = clock() - t;
cout << IND << t << endl;
test(res == -2);
